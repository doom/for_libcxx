#ifndef FOR_LIBCXX_CSTDINT
#define FOR_LIBCXX_CSTDINT

#include <stdint.h>

namespace std
{
    using ::uint8_t;
    using ::uint16_t;
    using ::uint32_t;
    using ::uint64_t;

    using ::int8_t;
    using ::int16_t;
    using ::int32_t;
    using ::int64_t;

    using ::uintptr_t;
    using ::intptr_t;
}

#endif /* !FOR_LIBCXX_CSTDINT */
