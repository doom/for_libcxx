#ifndef FOR_LIBCXX_CSTDDEF
#define FOR_LIBCXX_CSTDDEF

#include <stddef.h>

namespace std
{
    enum class byte : unsigned char
    {
    };

    using ::size_t;
    using ::ptrdiff_t;
    using ::max_align_t;

    using nullptr_t = decltype(nullptr);
}

#endif /* !FOR_LIBCXX_CSTDDEF */
